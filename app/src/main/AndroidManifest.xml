<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.forestmuseum">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:roundIcon="@drawable/head"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".LaunchActitity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".QuestionActivity" />
        <activity android:name=".TipActivity" />
        <activity android:name=".Unit01DetailActivity" />
        <activity android:name=".ItemFormatActivity" />
        <activity android:name=".ShowScoreActivity" />
        <activity android:name=".RegistActivity" />
        <activity android:name=".DialogActivity" />
        <activity android:name=".Unit03DetailActivity"></activity>
    </application>

</manifest>